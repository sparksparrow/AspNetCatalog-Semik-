@model ProductItem
@{
    List<Image> images = Model.Images as List<Image>;
    ViewBag.Title = Model.Title;
    ViewBag.Description = Model.MetaDescription;
    ViewBag.Keywords = Model.MetaKeywords;
}

<section class="bg-light">
    <div class="container pb-5">
        <div class="row">
            <div class="col-lg-5 mt-5">
                <div class="card mb-3">
                    @if (Model.Images.Any())
                    {
                            <img class="card-img img-fluid" src="~/images/ProductItems/@Model.Images.First().ProductItemId/@Model.Images.First().Name" alt="@Model.Images.First().Alt" id="product-detail"/>
                    }
                    else
                    {
                        <img class="card-img img-fluid" src="~/images/DefaultImages/Soon.png" alt="В процессе добавления" id="product-detail" />
                    }
                </div>
                <div class="row">
                    <div class="col-1 align-self-center">
                        <a href="#multi-item-example" role="button" data-bs-slide="prev">
                            <i class="text-dark fas fa-chevron-left"></i>
                            <span class="sr-only">Previous</span>
                        </a>
                    </div>
                    <div id="multi-item-example" class="col-10 carousel slide carousel-multi-item" data-bs-ride="carousel">
                        <div class="carousel-inner product-links-wap" role="listbox">

                            <div class="carousel-item active">
                                @if (images.Any())
                                {                                                                      
                                    @for (int i = 0; i < Model.Images.Count; i+=3)
                                    {
                                <div class="row">
                                    @if (i + 1 == Model.Images.Count)
                                    {
                                        <div class="col-4">
                                            <a href="#">
                                                <img class="card-img img-fluid" src="~/images/ProductItems/@images[i].ProductItemId/@images[i].Name" alt="@images[i].Alt" id="product-detail" />
                                            </a>
                                        </div>
                                    }
                                    else                                    
                                    @if (i + 2 == Model.Images.Count)
                                    {
                                        <div class="col-4">
                                            <a href="#">
                                                <img class="card-img img-fluid" src="~/images/ProductItems/@images[i].ProductItemId/@images[i].Name" alt="@images[i].Alt" id="product-detail" />
                                            </a>
                                        </div>
                                        <div class="col-4">
                                            <a href="#">
                                                <img class="card-img img-fluid" src="~/images/ProductItems/@images[i + 1].ProductItemId/@images[i + 1].Name" alt="@images[i + 1].Alt" id="product-detail" />
                                            </a>
                                        </div>
                                    }
                                    else
                                    {
                                    <div class="col-4">
                                        <a href="#">
                                            <img class="card-img img-fluid" src="~/images/ProductItems/@images[i].ProductItemId/@images[i].Name" alt="@images[i].Alt" id="product-detail" />
                                        </a>
                                    </div>
                                    <div class="col-4">
                                        <a href="#">
                                            <img class="card-img img-fluid" src="~/images/ProductItems/@images[i + 1].ProductItemId/@images[i + 1].Name" alt="@images[i + 1].Alt" id="product-detail" />
                                        </a>
                                    </div>
                                    <div class="col-4">
                                        <a href="#">
                                            <img class="card-img img-fluid" src="~/images/ProductItems/@images[i + 2].ProductItemId/@images[i + 2].Name" alt="@images[i + 2].Alt" id="product-detail" />
                                        </a>
                                    </div>
                                    }
                                </div>
                                    
                                }
                                }
                                </div>
                            </div>
                        </div>
                            <div class="col-1 align-self-center">
                                <a href="#multi-item-example" role="button" data-bs-slide="next">
                                    <i class="text-dark fas fa-chevron-right"></i>
                                    <span class="sr-only">Следующее</span>
                                </a>
                            </div>
                    </div>
            </div>
            <div class="col-lg-7 mt-5">
                <div class="card">
                    <div class="card-body">
                        <h1 class="h2">@Model.Name</h1>
                        @if (Model.Discount == 0)
                        {
                    <p class="h3 py-2">@Model.Price&#8381</p>
                        }
                        else
                        {
                    <p class="h3 py-2">@Model.TotalPrice&#8381</p>
                            <p class="h3 py-2">
                                <strike> @Model.Price&#8381 &nbsp;&nbsp;</strike> <font color="red">Скидка @Model.Discount% </font>
                            </p>
                        }
                        <h6>Описание:</h6>
                        <p>@Html.Raw(Model.Description)</p>
                        <input type="hidden" name="product-title" value="Activewear">
                        <div class="row">
                            <div class="col-auto">
                                <ul class="list-inline pb-3">
                                    <li class="list-inline-item">
                                        Размеры :
                                    </li>
                                    @if (Model.Sizes.Any())
                                    {
                                        @foreach (Size size in Model.Sizes)
                                        {
                                            <li class="list-inline-item"><span class="page-item disabled">@size.SizeValue</span></li>
                                        }
                                    }
                                </ul>
                            </div>
                            <div class="col-auto">
                            </div>
                        </div>
                        <div class="row pb-3">
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
